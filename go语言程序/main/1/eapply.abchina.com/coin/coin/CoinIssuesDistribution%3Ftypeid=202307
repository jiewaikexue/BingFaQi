    $(document).ready(function () {
        if (loginState == 1) {
            $('#login_no').hide();
            if (removeLS) {
                $('#btnOut').hide();
            }
            else
                $('#btnOut').attr('href', logoutUrl)
        }
        else {
            $('#login_yes').hide();
            if (removeLS) {
                $('#btnIn').hide();
            }
            else
                $('#btnIn').attr('href', loginUrl)
        }
       
    });
