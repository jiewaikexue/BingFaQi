        <script type="text/javascript">

            function timer(intDiff,index){
                    $("#timeDifftr"+index).show();
                    var timerId = window.setInterval(function() {
	                var day = 0,
		                hour = 0,
		                minute = 0,
		                second = 0; //时间默认值
	                if (intDiff > 0) {
		                day = Math.floor(intDiff / (60 * 60 * 24));
                        hour = Math.floor(intDiff / (60 * 60)) - (day * 24);
	                    minute = Math.floor(intDiff / 60) - (day * 24 * 60) - (hour * 60);
	                    second = Math.floor(intDiff) - (day * 24 * 60 * 60) - (hour * 60 * 60) - (minute * 60);
	                }
	                if (hour <= 9)
		                hour = '0' + hour; //小于9的时候给小时添加一个0例如00:13:14
	                if (minute <= 9)
		                minute = '0' + minute;
	                if (second <= 9)
		                second = '0' + second;

                    $("#countdown"+index).text("倒计时：")
                    $("#day_show"+index).html('<span id="day_show">' + day + '天</span>');
                    $("#hour_show"+index).html('<span id="hour_show">' + hour + '小时</span>');
                    $("#minute_show"+index).html('<span id="minute_show">' + minute + '分钟</span>');
                    $("#second_show"+index).html('<span id="second_show">' + second + '秒</span>');

	                if (Number(hour) == 0 && Number(minute) == 0 & Number(second) == 0){
                        clearInterval(timerId); //时分秒均为0的时候清除定时器
                        //隐藏倒计时
                        $("#timeDifftr"+index).hide();
                    }
                    intDiff--; // 秒数-1
                }, 1000);
            }

            $(function(){
                var timeDiffList = "[&quot;-1&quot;,&quot;-1&quot;]";
                timeDiffList = timeDiffList.replace(/&quot;/g, '"');
                var timeDiffListObj = new Array();
                if(timeDiffList != ""){
                    timeDiffListObj = JSON.parse(timeDiffList);
                }
                for (var i = 0; i < timeDiffListObj.length; i++) {
                    var timeDiff = parseInt(timeDiffListObj[i]);//倒计时总秒数量
                    if(timeDiff > 0){
                        timer(timeDiff, i);
                    }
                }

            });


    function notification(issueid) {
        $("#"+ issueid+ "").dialog(
      {
          width: 330,
          draggable: false,
          modal: true,
          position: { 'center': 'top' },
          open: function (event, ui) {
              $(".ui-dialog-titlebar-close", $(this).parent()).hide();
          }


      });
    }

    function Verification(issueid) {
        window.location.href = "/coin/coin/OrdinaryCoinRequest?issueid=" + issueid +"&batchOrder=1&t=" + new Date().getTime();
    }
    function VerificationWithBatch(issueid,batchorder) {
        window.location.href = "/coin/coin/OrdinaryCoinRequest?issueid=" + issueid +"&batchOrder= "+batchorder+"&t=" + new Date().getTime();
    }
    function Close() {
        window.location.href = "/coin/";

              //  javascript:history.go(-1);

            }


