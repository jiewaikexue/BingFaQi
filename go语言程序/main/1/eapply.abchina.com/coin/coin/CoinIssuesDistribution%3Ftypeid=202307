        //注册事件
        function MemberRegisterDialog() {
            showDialogbyUrl("https://app.abchina.com.cn/static/sns/RegisterGuide/", 440);
        }
        function showDialogbyUrl(url, dialogHeight) {
            //viewWidth 视口宽度，viewHeight 视口高度 兼容常见浏览器
            var viewWidth = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;
            var viewHeight = window.innerHeight || document.documentElement.clientHeight || document.body.clientHeight;
            var iFrameWidth = 900; //iframe 盒子的宽
            var iFrameHeight = 460; //iframe 盒子的高
            var yOffset = (viewHeight - iFrameHeight) / 2;  //偏移量换算公式
            var xOffset = (viewWidth - iFrameWidth) / 2; //偏移量换算公式

            $('<iframe id="frm" frameborder="0"  style="padding-left:12px; padding-top:10px;background:#fff;" scrolling="no" src="' + url + '" />').dialog({
                width: 900,
                title: '注册',
                modal: true,
                height: dialogHeight,
                position: [xOffset, yOffset],
                draggable: false,//不能拖动
                closeOnEscape: true,
                resizable: false
            }).width(900).height(dialogHeight);

        }
        function Brancheapply() {
            window.location.href = "/coin/coin/DefaultQuery";
        }
